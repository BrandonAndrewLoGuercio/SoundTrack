  <div class="container">
      <div class="col-sm-4">
          <p id="notice"><%= notice %></p>
          <h1 class="lead">Listing posts</h1>

          <table>
            <thead>
              <tr>
                <th colspan="3"></th>
              </tr>
            </thead>

      <tbody>
        <% @posts.each do |post| %>
          <tr>
            <td><%= post.message %></td>
            <td><a class="btn icon-btn btn-info"<%= link_to 'Show', post %></a></td>
            <td><a class="btn icon-btn btn-warning"<%= link_to 'Edit', edit_post_path(post) %></a></td>
            <td><a class="btn icon-btn btn-danger"<%= link_to 'Remove', post, method: :delete, data: { confirm: 'Are you sure?' } %></a></td>
          </tr>
        <% end %>
       </tbody>
      </table>
        <br>
        <a class="btn icon-btn btn-success" <%= link_to'New Post', new_post_path%> </a>
      </div>

