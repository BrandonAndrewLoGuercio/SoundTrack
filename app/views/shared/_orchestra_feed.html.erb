<div id="feed" class="col-sm-8">
  <h1 class="lead">Orchestra Feed</h1>

  <ul>
    <% @posts.each do |post| %>
        <li>
          <%= link_to post.user.username, user_path(post.user.id) %>
          <%= post.post_title %>
          <iframe id="new_post_vid" class="video w100" width="640" height="360" src="//www.youtube.com/embed/<%= post.video_id %>" frameborder="0" allowfullscreen></iframe><br>
          Description: <%= post.description %><br><br>
        </li>
    <% end %>
  </ul>

</div>s