  <head>
    <meta charset="utf-8">
    <title>SoundTrack</title>
  </head>
  <body>
   <div class="jumbotron" id="background">
    <div class="container-fluid">
      <% if !user_signed_in? %>
        <div class="jumbotron">
          <h1>Create your SoundTrack</h1>
          <p>Music and filler words and stuff and shorts XOXO Vice Austin cardigan. Organic drinking vinegar freegan pickled.</p>
          <p><%= link_to "Sign Up", new_user_registration_path, class: "btn btn-info btn-block" %></p>
          <p><%= link_to 'Sign in', new_user_session_path, class: "btn btn-primary btn-block" %></p>
        </div>
      <% end %>
    </div>
<div class="jumbotron", id="background2">
     </div>

  <div class="container">
    <h1>SoundTrack!</h1>
    <% if user_signed_in? %>
        <% if false && current_user.avatar.present? #TODO when avatar is working  %>
            <%= image_tag current_user.avatar_url.to_s %><br><br>
        <% end %>

          <% current_user.username %>
        <%= render 'welcome/homefeed' %>
    <% else %>
        <ul>
          <li><%= link_to 'Sign up', new_user_registration_path %></li>
          <li><%= link_to 'Sign in', new_user_session_path %></li>
          <%= link_to user_omniauth_authorize_path(:facebook) do %>
              <%= image_tag "facebook_button.png" %>
          <% end %>
        </ul>
        <!--<%= render 'landingpage' %>-->
    <% end %>
  </div>
  </div>

  </body>
