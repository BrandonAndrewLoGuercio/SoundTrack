  <head>
    <meta charset="utf-8">
    <title>SoundTrack</title>
  </head>
  <body>
    <div class="container-fluid">
    <div class="center jumbotron">
      <% if !user_signed_in? %>
          <h2>Create your SoundTrack</h2>
          <p></p>
          <p><%= link_to "Sign Up", new_user_registration_path, class: "btn btn-info btn-block" %></p>
          <p><%= link_to 'Sign in', new_user_session_path, class: "btn btn-primary btn-block" %></p>
          <%= link_to user_omniauth_authorize_path(:facebook) do %>
              <%= image_tag "facebook_button.png" %>
          <% end %>
      <% end %>
      </div>
      </div>
    <% if user_signed_in? %>

       <% if false && current_user.avatar.present? #TODO when avatar is working  %>
            <%= image_tag current_user.avatar_url.to_s %><br><br>
        <% end %>

          <% current_user.username %>
        <%= render 'welcome/homefeed' %>
    <% else %>
        <%= render 'landingpage' %>
    <% end %>
  </div>

  </body>
